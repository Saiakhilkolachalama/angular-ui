pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build'
                sh 'npm run build'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploy'

                sh "aws s3 sync dist/angular-demo/ s3://s3forjenkinsbuild123"
            }
        }
    }
}
